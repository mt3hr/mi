{
  "name": "mi",
  "version": "1.1.4",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "go_mod": "rimraf src/app/go.mod && rimraf src/app/go.sum && cd src/app && go mod init github.com/mt3hr/mi/src/app && cd mi/main/mi && go mod tidy",
    "clean_app_embed": "rimraf src/app/mi/mi/embed/html",
    "copy_dist_to_app_embed": "mkdirp src/app/mi/mi/embed/html && copyfiles -u 1 \"dist/**/*\" src/app/mi/mi/embed/html",
    "preparate_install": "npm run clean_app_embed && npm run build && npm run copy_dist_to_app_embed",
    "install_app": "npm run go_mod && npm run preparate_install && cd src/app/mi/main/mi && go install -ldflags \"-H windowsgui\"",
    "install_server": "npm run go_mod && npm run preparate_install && cd src/app/mi/main/mi_server && go install",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@vuepic/vue-datepicker": "^4.2.3",
    "copyfiles": "^2.4.1",
    "core-js": "^3.8.3",
    "mkdirp": "^3.0.1",
    "move-file": "^3.1.0",
    "rimraf": "^5.0.0",
    "roboto-fontface": "*",
    "vue": "^3.2.13",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "cpx": "^1.5.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "sass": "^1.55.0",
    "typescript": "^4.0.0",
    "vite": "^3.0.9",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-tsc": "^1.0.9"
  }
}
